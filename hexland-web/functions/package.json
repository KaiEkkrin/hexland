  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/storage": "^5.4.0",
    "@types/child-process-promise": "^2.2.1",
    "@types/uuid": "^8.3.0",
    "child-process-promise": "^2.2.1",
    "dayjs": "^1.8.36",
    "firebase-admin": "^9.3.0",
    "firebase-functions": "^3.13.0",
    "fluent-iterable": "^0.1.6",
    "rxjs": "^6.6.3",
    "three": "^0.120.0",
    "uuid": "^8.3.0",
    "webdav": "^3.5.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0",
    "tslint": "^5.12.0",
    "typescript": "^3.8.0"
  },
